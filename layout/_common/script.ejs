<script src="//cdn.jsdelivr.net/npm/jquery@1.12.4/dist/jquery.min.js"></script>
<script src="//cdn.bootcss.com/highlight.js/9.2.0/highlight.min.js"></script>
<script>
window.jQuery || document.write('<script src="js/jquery.min.js"><\/script>')
   hljs.initHighlightingOnLoad();
</script>
<%- js('js/plugin.min') %>
<%- js('js/application') %>
<%- partial('_script/search') %>
<% if (page.layout === 'repository'){ %>
   <%- partial('_script/repository') %>
<% } %>
<% if (page.layout === 'books'){ %>
   <%- partial('_script/douban') %>
<% } %>
<% if (is_home() || is_post()) { %>
   <%- partial('_script/pv') %>
<% } %>
<% if (!is_home() && !is_category() && !is_tag() && !is_archive() && post.comments) { %>
   <%- partial('_script/comment') %>
<% } %>
<%- partial('_script/fancybox') %>
<%- partial('_script/analytics') %>